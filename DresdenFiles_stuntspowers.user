    <evalrule phase="Validate" message="????"><![CDATA[var foundcount as number
var supercost as number

foreach pick in hero from Stunt where "Supernat.Supernat"
  foundcount += 1
  supercost += eachpick.field[stRefCost].value * eachpick.field[stNumber].value
nexteach

@valid = 1
if (foundcount <> 1) then
   @valid = 0
   @message = "Requires 1 supernatural power.  You have " & foundcount & "."
elseif (supercost < 1) then
   @valid = 0
   @message = "Supernatural power must cost at least -1 Refresh.  Yours costs " & supercost & "."
elseif (supercost > 2) then
   @valid = 0
   @message = "Supernatural power must cannot exceed -2 Refresh.  Yours costs -" & supercost & "."
endif]]></evalrule>
