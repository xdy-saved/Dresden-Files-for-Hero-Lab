  <thing id="pwrRefinIt" name="P/SC: Refinement: Item Slots" description="{b}Source:{/b} Dresden Files RPG" compset="Stunt">
    <usesource source="srcDresden" />
    <tag group="Hide" tag="Tracker" name="Tracker" abbrev="Tracker" />
    <tag group="AssocSkill" tag="skAACustom" />
    <tag group="Supernat" tag="Spellcraft" />
    <tag group="Supernat" tag="Supernat" />
    <tag group="Helper" tag="ShowIncr" />
    <evalrule phase="Validate" message="????"><![CDATA[var slots as number
var maxslots as number

slots = 0
foreach pick in hero from Stunt where "thingid.pwrFocItem"
   slots += eachpick.field[stNumber].value * 2
nexteach

foreach pick in hero from Stunt where "thingid.pwrEnchItm"
   slots += eachpick.field[stNumber].value
nexteach

maxslots = 0
if (hero.tagis[Stunt.pwrEvocati] <> 0) then
   maxslots += 4
endif

if (hero.tagis[Stunt.pwrThaumat] <> 0) then
   maxslots += 4
endif

if (hero.tagis[Stunt.pwrChannel] <> 0) then
   maxslots += 4
endif

if (hero.tagis[Stunt.pwrRitual] <> 0) then
   maxslots += 4
endif

if (hero.tagis[Stunt.pwrRefinIt] <> 0) then
   maxslots += 4
endif

validif (maxslots = slots) 

if (maxslots > slots) then
   @message = "You have underspent on Focus/Enchanted Item slots."
elseif (maxslots < slots) then
   @message = "You have overspent on Focus/Enchanted Item slots."
endif]]></evalrule>
    <pickreq thing="tmpMortal" ispreclude="yes" />
    <exprreq message="Requires Evocation or Thaumaturgy."><![CDATA[hero.tagis[Stunt.pwrEvocati] + hero.tagis[Stunt.pwrThaumat] <> 0]]></exprreq>
  </thing>
