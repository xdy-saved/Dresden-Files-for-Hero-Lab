  <thing id="tmpMnrTal" name="Template: Minor Talent" description="{b}Source:{/b} Dresden Files RPG" compset="Stunt" uniqueness="unique">
    <fieldval field="stRefCost" value="0" />
    <usesource source="srcDresden" />
